<Application x:Class="WpfPartialViewApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="WindowBrush" Color="#E1CF9F">
            </SolidColorBrush>
            <LinearGradientBrush x:Key="HeaderBrush"  StartPoint="0,0" EndPoint="0,1">
                <GradientStop Color="#C6A448" Offset="0" />
                <GradientStop Color="#E1CF9F" Offset="1" />
            </LinearGradientBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>
